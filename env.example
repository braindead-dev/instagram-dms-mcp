# Instagram Cookies (required)
# Copy these values from instagram.com > DevTools > Application > Cookies

IG_SESSION_ID=your_sessionid_here
IG_USER_ID=your_ds_user_id_here
IG_CSRF_TOKEN=your_csrftoken_here

# Optional but recommended
IG_DATR=your_datr_here
IG_DID=your_ig_did_here
IG_MID=your_mid_here

# Server port (optional, default 8000)
PORT=8000
